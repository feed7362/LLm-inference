{
  "name": "Inference GPU DevContainer",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "inference",
  runServices: [
    "inference",
    "postgres"
  ],
  "forwardPorts": [5432, 2222],
  "postCreateCommand": "nvidia-smi",
  "customizations": {
    "jetbrains": {
      "backend": "PyCharm"
    }
  }
}